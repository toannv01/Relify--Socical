<p id="notice"><%= notice %></p>

<h1>Add Friends</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @add_friends.each do |add_friend| %>
      <tr>
        <td><%= link_to 'Show', add_friend %></td>
        <td><%= link_to 'Edit', edit_add_friend_path(add_friend) %></td>
        <td><%= link_to 'Destroy', add_friend, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Add Friend', new_add_friend_path %>
